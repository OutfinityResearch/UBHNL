Proof MinorCannotVote:
    Given:
        Age(p1, 16).
        For all Person p: If Age($p, 16) then Minor($p).
        For all Person p: If Minor($p) then CannotVote($p).
    Apply:
        Rule 1 on p1.
    Derive:
        Minor(p1).
    Apply:
        Rule 2 on p1.
    Derive:
        CannotVote(p1).
    Therefore:
        CannotVote(p1).
